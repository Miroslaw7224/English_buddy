{
  "name": "english-agent",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "deps-check": "npx depcheck",
    "audit": "npm audit --audit-level=moderate",
    "check-all": "npm run type-check && npm run deps-check && npm run audit",
    "qa:validate": "tsx quality/scripts/validate-flashcards.ts data/flashcards",
    "qa:smoke": "tsx quality/scripts/smoke-flashcards.ts data/flashcards",
    "qa": "npm run qa:validate && npm run qa:smoke",
    "db:migrate:quality": "npx supabase db push --include-all",
    "qa:report:fields": "npx supabase db push --include-all && echo 'Fields sanity check completed'",
    "qa:report:dups": "npx supabase db push --include-all && echo 'Near duplicates check completed'",
    "qa:report:srs": "npx supabase db push --include-all && echo 'SRS inconsistencies check completed'",
    "qa:report:lang": "npx supabase db push --include-all && echo 'Language IPA issues check completed'"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@tanstack/react-query": "^5.90.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.63.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^11.0.4",
    "zod": "^4.1.11",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5"
  }
}
